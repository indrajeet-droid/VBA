Sub for_loop()

Dim x As Integer

'condition 10(10 or less than 10)
'step 2 increment by 2
For x = 1 To 10 step 2
   
    MsgBox x
    
Next

End Sub


```````````````````````````````````````````````````
Sub for_loop()

Dim x As Integer

For x = 1 To 10
   
    Cells(x, 1).Value = 10
    
Next

End Sub

```````````````````````````````````````````````````````
Sub for_loop()

Dim x As Integer

For x = 1 To 10
   
    Cells(x, 1).Value = x
    
Next

End Sub

