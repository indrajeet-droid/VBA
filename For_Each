Sub worksheets_1()

Dim ws As Worksheet

For Each ws In Worksheets

    Debug.Print ws.Name
Next ws


End Sub



`````````````````````````````````````````````````````
Sub for_each_next_1()

Dim sht As Worksheet

For Each sht In Worksheets

    MsgBox sht.Name

Next


End Sub

```````````````````````````````````
Sub for_each_next_1()

Dim sht As Worksheet

For Each sht In ThisWorkbook.Sheets

    MsgBox sht.Name

Next


End Sub
